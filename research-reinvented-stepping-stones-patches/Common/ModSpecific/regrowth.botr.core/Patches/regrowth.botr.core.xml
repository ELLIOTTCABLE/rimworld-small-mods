<?xml version="1.0" encoding="utf-8"?>
<Patch>

   <Operation Class="PatchOperationFindMod">
      <mods>
         <li>ReGrowth: Core</li>
      </mods>
      <match Class="PatchOperationSequence">
         <operations>

            <li Class="XmlExtensions.PatchOperationSafeAdd">
               <xpath>
                  Defs/ResearchProjectDef[defName = "RG_MushroomCultivation"
                     or defName = "RG_BerryCultivation"]
               </xpath>
               <safetyDepth>1</safetyDepth>
               <value>
                  <prerequisites>
                     <li>RR_Agriculture</li>
                  </prerequisites>
               </value>
            </li>

         </operations>
      </match>      <!-- PatchOperationSequence -->
   </Operation>   <!-- PatchOperationFindMod: ReGrowth: Core -->

</Patch>
